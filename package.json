{
  "name": "rxjs-presentation",
  "version": "1.0.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "description": "Presenting reactive programming basics with RxJS",
  "main": "server.js",
  "scripts": {
    "live-css": "cross-env NODE_ENV=dev-server FONT_AWESOME='css' webpack-dev-server --mode development",
    "live-svg": "cross-env NODE_ENV=dev-server FONT_AWESOME='svg' webpack-dev-server --mode development",
    "dev-css": "cross-env NODE_ENV=development FONT_AWESOME='css' webpack --mode development",
    "dev-svg": "cross-env NODE_ENV=development FONT_AWESOME='svg' webpack --mode development",
    "build-css": "cross-env NODE_ENV=production FONT_AWESOME='css' webpack --mode production",
    "build-svg": "cross-env NODE_ENV=production FONT_AWESOME='svg' webpack --mode production",
    "build-web": "cross-env NODE_ENV='production-web-css' FONT_AWESOME='css' webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falcoprescher/RxJS-Presentation.git"
  },
  "keywords": [
    "RxJS",
    "Reactive",
    "Programming",
    "Dataflow",
    "Pull",
    "Stream",
    "JavaScript"
  ],
  "author": "Falco Prescher",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/falcoprescher/RxJS-Presentation/issues"
  },
  "homepage": "https://github.com/falcoprescher/RxJS-Presentation#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "copy-webpack-plugin": "^4.5.3",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-node": "^8.0.0",
    "file-loader": "^1.1.11",
    "google-fonts-plugin": "^2.0.2",
    "html-webpack-include-assets-plugin": "^1.0.6",
    "html-webpack-plugin": "^3.2.0",
    "mathjax-node-page": "^3.0.1",
    "mini-css-extract-plugin": "^0.4.4",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.4.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-brands-svg-icons": "^5.4.1",
    "@fortawesome/free-regular-svg-icons": "^5.4.1",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "highlight.js": "^9.12.0",
    "reveal.js": "^3.7.0",
    "reveal.js-menu": "^1.2.0",
    "reveal_external": "^1.3.0",
    "reveald3": "^1.4.6"
  }
}
